{
  "expo": {
    "name": "fiapFilmes",
    "slug": "fiapFilmesMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF0066"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.olegzera.FiapFilmes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "FF.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
